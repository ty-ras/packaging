    "eslint": "8.50.0",
    "eslint-plugin-jsdoc": "46.8.2",
    "eslint-plugin-type-only-import": "0.9.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-sonarjs": "0.21.0",
    "isomorphic-git": "1.24.5",
    "madge": "6.1.0",
    "prettier": "3.0.3",
    "tar": "6.2.0",
    "ts-node": "10.9.1",
    "typedoc": "0.25.1",
    "typescript": "5.2.2",
    "undici": "5.25.4",
    "vite": "4.4.9",
    "vite-plugin-solid": "2.7.0",
    "@suid/vite-plugin": "0.1.5"
  },
  "scripts": {
    "build": "vite build",
    "build:clean": "yarn run clear-build-artifacts && yarn run build",
    "generate-structure-and-versions": "ts-node --project tsconfig.scripts.json scripts",
    "dev": "vite",
    "preview": "vite preview",
    "clear-build-artifacts": "rm -rf dist build",
    "lint": "yarn run lint:eslint && yarn run lint:circular",
    "lint:circular": "madge --circular --no-color --no-spinner --extensions ts --warning ./src",
    "lint:eslint": "eslint ./src --ext .ts,.tsx",
    "tsc": "tsc --project tsconfig.build.json",
    "tsc:plain": "tsc",
    "test:coverage": "c8 --temp-directory /tmp ava",
    "test:run": "c8 --temp-directory /tmp --reporter text ava"
  },
  "resolutions": {
    "dependency-tree": "10.0.9",
    "detective-typescript": "11.1.0",
    "precinct": "11.0.5",
    "typescript": "5.2.2"
  }
}